        <nl:gfbr>50</nl:gfbr>
        <nl:mfbr>100</nl:mfbr>
        <nl:pdb>20</nl:pdb>
        <nl:per>0</nl:per>
      </nl:network-layer-link-attributes>
    </nt:link>


    <nl:network-layer-topology xmlns:nl="urn:ietf:params:xml:ns:yang:network-layer">
      <nl:element>element3</nl:element>
    </nl:network-layer-topology>
  </network>

