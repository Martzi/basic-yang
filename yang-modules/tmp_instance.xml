<networks
	xmlns="urn:ietf:params:xml:ns:yang:ietf-network">
	<?xml version="1.0" ?>
	<!-- Intel RealSense D455 network topology -->
	<network>
		<network-id>camera-network</network-id>
		<network-types>
			<dl:device-layer-topology
				xmlns:dl="urn:ietf:params:xml:ns:yang:device-layer"/> <!-- device layer-be nincs supporting! -->
			</network-types>
			<node>
				<node-id>camera-usb-typec-node</node-id>
				<dl:device-layer-node-attributes
					xmlns:dl="urn:ietf:params:xml:ns:yang:device-layer">
					<dl:device-name>camera-0</dl:device-name>
					<dl:device-type>camera</dl:device-type>
				</dl:device-layer-node-attributes>
				<ietf-network-topology:termination-point
					xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
					<ietf-network-topology:tp-id>tp-1</ietf-network-topology:tp-id>
					
                    <device-layer:device-layer-termination-point-attributes
						xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
						<device-layer:port-name>port-1</device-layer:port-name>
						<device-layer:port-type>usb-c</device-layer:port-type>
						<usb-port:usb-port
							xmlns:usb-port="urn:ietf:params:xml:ns:yang:usb-port">
							<usb-port:port-id>usb-typec-0</usb-port:port-id>
							<usb-port:version>USB3.2</usb-port:version>
							<usb-port:speed>5.00</usb-port:speed>
							<usb-port:power-consumption>2.5</usb-port:power-consumption>
						</usb-port:usb-port>
					</device-layer:device-layer-termination-point-attributes>
				</ietf-network-topology:termination-point>
			</node>
		<!-- ethernet-0 -->
		<!-- USB-30-0 -->
		<!-- USB-30-1 -->
		<!-- NUC network topology -->
            <node>
                <node-id>nuc-0-usb-node</node-id>
                <ietf-network-topology:termination-point
                    xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
                    <ietf-network-topology:tp-id>tp-1</ietf-network-topology:tp-id>
                    <dl:device-layer-termination-point-attributes
                        xmlns:dl="urn:ietf:params:xml:ns:yang:device-layer">
                        <dl:port-name>port-1</dl:port-name>
                        <dl:port-type>usb-a</dl:port-type>
                        <usb:usb-port
                            xmlns:usb="urn:ietf:params:xml:ns:yang:usb-port">
                            <usb:port-id>USB30-0</usb:port-id>
                            <usb:version>USB3.0</usb:version>
                            <usb:speed>5.00</usb:speed>
                            <usb:power-consumption>2.5</usb:power-consumption>
                        </usb:usb-port>
                    </dl:device-layer-termination-point-attributes>
                </ietf-network-topology:termination-point>
            </node>
            <node>
                <node-id>nuc-0-cat6-node</node-id>
                <dl:device-layer-node-attributes
                    xmlns:dl="urn:ietf:params:xml:ns:yang:device-layer">
                    <dl:device-name>nuc-0</dl:device-name>
                    <dl:device-type>pc</dl:device-type>
                </dl:device-layer-node-attributes>
                <ietf-network-topology:termination-point
                    xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
                    <ietf-network-topology:tp-id>tp-1</ietf-network-topology:tp-id>
                    <device-layer:device-layer-termination-point-attributes
                        xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
                        <device-layer:port-name>rj45-0</device-layer:port-name>
                        <device-layer:port-type>rj45</device-layer:port-type>
                        <ethernet-port:ethernet-port
                            xmlns:ethernet-port="urn:ietf:params:xml:ns:yang:ethernet-port">
                            <ethernet-port:port-id>eth-0</ethernet-port:port-id>
                            <ethernet-port:mac-address>00:1A:2B:3C:4D:5E</ethernet-port:mac-address>
                            <ethernet-port:speed>1Gbps</ethernet-port:speed>
                            <ethernet-port:duplex-mode>full</ethernet-port:duplex-mode>
                        </ethernet-port:ethernet-port>
                    </device-layer:device-layer-termination-point-attributes>
                </ietf-network-topology:termination-point>
            </node>
            <ietf-network-topology:link
                xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
                <ietf-network-topology:link-id>link-1</ietf-network-topology:link-id>
                <ietf-network-topology:source>
                    <ietf-network-topology:source-node>node-1</ietf-network-topology:source-node>
                    <ietf-network-topology:source-tp>tp-1</ietf-network-topology:source-tp>
                </ietf-network-topology:source>
                <ietf-network-topology:destination>
                    <ietf-network-topology:dest-node>node-2</ietf-network-topology:dest-node>
                    <ietf-network-topology:dest-tp>tp-2</ietf-network-topology:dest-tp>
                </ietf-network-topology:destination>
                <device-layer:device-layer-link-attributes
                    xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
                    <device-layer:physical-link-name>Link-1</device-layer:physical-link-name>
                    <device-layer:physical-link-type>cat5e</device-layer:physical-link-type>
                </device-layer:device-layer-link-attributes>
            </ietf-network-topology:link>
        </network>
    </networks>