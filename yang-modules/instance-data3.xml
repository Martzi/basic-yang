
<networks xmlns="urn:example:network-topology">
  
  <network>
    
    <network-id>network-1</network-id>
    
    <network-types>
      <al:application-layer-topology xmlns:al="urn:example:al"/>
      <dl:device-layer-topology xmlns:dl="urn:example:dl"/>
      <nl:network-layer-topology xmlns:nl="urn:example:nl"/>
    </network-types>
    
    <supporting-network>
      <network-ref>supporting-network-1</network-ref>
    </supporting-network>
    
    <node>
      <node-id>node-1</node-id>
      
      <supporting-node>
        <network-ref>supporting-network-1</network-ref>
        <node-ref>supporting-node-1</node-ref>
      </supporting-node>
      
      <al:application-layer-node-attributes xmlns:al="urn:example:al">
        <al:component-name>Component-1</al:component-name>
        <al:component-type>TypeA</al:component-type>
      </al:application-layer-node-attributes>
      
      <dl:device-layer-node-attributes xmlns:dl="urn:example:dl">
        <dl:device-name>Device-1</dl:device-name>
        <dl:device-type>TypeB</dl:device-type>
      </dl:device-layer-node-attributes>
      
      <nt:termination-point xmlns:nt="urn:example:nt">
        <nt:tp-id>tp-1</nt:tp-id>
        
        <nt:supporting-termination-point>
          <nt:network-ref>supporting-network-1</nt:network-ref>
          <nt:node-ref>supporting-node-1</nt:node-ref>
          <nt:tp-ref>tp-ref-1</nt:tp-ref>
        </nt:supporting-termination-point>
        
        <al:application-layer-termination-point-attributes xmlns:al="urn:example:al">
          <al:reference-interface-name>eth0</al:reference-interface-name>
          <al:reference-interface-type>ethernet</al:reference-interface-type>
        </al:application-layer-termination-point-attributes>
        
        <dl:device-layer-termination-point-attributes xmlns:dl="urn:example:dl">
          <dl:port-name>port-1</dl:port-name>
          <dl:port-type>
            <eth:ethernet-port xmlns:eth="urn:example:eth">
              <eth:port-id>eth1</eth:port-id>
              <eth:mac-address>00:1A:2B:3C:4D:5E</eth:mac-address>
              <eth:speed>1000Mbps</eth:speed>
              <eth:duplex-mode>full</eth:duplex-mode>
            </eth:ethernet-port>
          </dl:port-type>
        </dl:device-layer-termination-point-attributes>
        
        <nl:network-layer-termination-point-attributes xmlns:nl="urn:example:nl">
          <nl:interface-name>GigabitEthernet0/1</nl:interface-name>
          <nl:interface-type>gigabitEthernet</nl:interface-type>
          <nl:ip-address>192.168.1.1</nl:ip-address>
        </nl:network-layer-termination-point-attributes>
      </nt:termination-point>

      <nl:network-layer-node-attributes xmlns:nl="urn:example:nl">
        <nl:node-name>Node-A</nl:node-name>
        <nl:node-type>router</nl:node-type>
      </nl:network-layer-node-attributes>

    </node>

    <nt:link xmlns:nt="urn:example:nt">

      <nt:link-id>link-1</nt:link-id>

      <nt:source>
        <nt:source-node>node-1</nt:source-node>
        <nt:source-tp>tp-1</nt:source-tp>
      </nt:source>

      <nt:destination>
        <nt:dest-node>node-2</nt:dest-node>
        <nt:dest-tp>tp-2</nt:dest-tp>
      </nt:destination>

      <nt:supporting-link>
        <nt:network-ref>supporting-network-1</nt:network-ref>
        <nt:link-ref>supporting-link-1</nt:link-ref>
      </nt:supporting-link>

      <al:application-layer-link-attributes xmlns:al="urn:example:al">
        <al:logical-link-name>Link-A</al:logical-link-name>
        <al:logical-link-type>logical</al:logical-link-type>
        <al:bitrate>10</al:bitrate>
      </al:application-layer-link-attributes>

      <dl:device-layer-link-attributes xmlns:dl="urn:example:dl">
        <dl:physical-link-name>Link-1</dl:physical-link-name>
        <dl:physical-link-type>fiber</dl:physical-link-type>
      </dl:device-layer-link-attributes>

      <nl:network-layer-link-attributes xmlns:nl="urn:example:nl">
        <nl:link-name>Link-B</nl:link-name>
        <nl:link-type>ip-link</nl:link-type>
        <nl:_5qi>9</nl:_5qi>
        <nl:gfbr>100</nl:gfbr>
        <nl:mfbr>200</nl:mfbr>
        <nl:pdb>10</nl:pdb>
        <nl:per>0.1</nl:per>
      </nl:network-layer-link-attributes>

    </nt:link>

  </network>

</networks>
