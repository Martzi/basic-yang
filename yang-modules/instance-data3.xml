<networks xmlns="urn:ietf:params:xml:ns:yang:ietf-network">
  <network>
    <network-id>network-1</network-id>
    <network-types>
      <device-layer:device-layer-topology xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer"/>
      <network-layer:network-layer-topology xmlns:network-layer="urn:ietf:params:xml:ns:yang:network-layer"/>
    </network-types>
   
    <supporting-network>
      <network-ref>supporting-network-1</network-ref>
    </supporting-network>
    
    <node>
      <node-id>node-1</node-id>
      
      <supporting-node>
        <network-ref>supporting-network-1</network-ref>
        <node-ref>supporting-node-1</node-ref>
      </supporting-node>
      

      <device-layer:device-layer-node-attributes xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
        <device-layer:device-name>Device-1</device-layer:device-name>
        <device-layer:device-type>camera</device-layer:device-type>
      </device-layer:device-layer-node-attributes>
      
      <ietf-network-topology:termination-point xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
        <ietf-network-topology:tp-id>tp-1</ietf-network-topology:tp-id>
        <ietf-network-topology:supporting-termination-point>
          <ietf-network-topology:network-ref>supporting-network-1</ietf-network-topology:network-ref>
          <ietf-network-topology:node-ref>supporting-node-1</ietf-network-topology:node-ref>
          <ietf-network-topology:tp-ref>tp-ref-1</ietf-network-topology:tp-ref>
        </ietf-network-topology:supporting-termination-point>
        
        <device-layer:device-layer-termination-point-attributes xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
          <device-layer:port-name>port-1</device-layer:port-name>
          <device-layer:port-type>rj45</device-layer:port-type>
          <ethernet-port:ethernet-port xmlns:ethernet-port="urn:ietf:params:xml:ns:yang:ethernet-port">
            <ethernet-port:port-id>rj45</ethernet-port:port-id>
            <ethernet-port:mac-address>00:1A:2B:3C:4D:5E</ethernet-port:mac-address>
            <ethernet-port:speed>1Gbps</ethernet-port:speed>
            <ethernet-port:duplex-mode>full</ethernet-port:duplex-mode>
          </ethernet-port:ethernet-port>
          
        </device-layer:device-layer-termination-point-attributes>
        <network-layer:network-layer-termination-point-attributes xmlns:network-layer="urn:ietf:params:xml:ns:yang:network-layer">
          <network-layer:interface-name>GigabitEthernet01</network-layer:interface-name>
          <network-layer:interface-type>ip</network-layer:interface-type>
          <network-layer:ip-address>192.168.1.1</network-layer:ip-address>
        </network-layer:network-layer-termination-point-attributes>
      </ietf-network-topology:termination-point>
      
      <network-layer:network-layer-node-attributes xmlns:network-layer="urn:ietf:params:xml:ns:yang:network-layer">
        <network-layer:node-name>Node-A</network-layer:node-name>
        <network-layer:node-type>generic</network-layer:node-type>
      </network-layer:network-layer-node-attributes>
    </node>
    
    <ietf-network-topology:link xmlns:ietf-network-topology="urn:ietf:params:xml:ns:yang:ietf-network-topology">
      <ietf-network-topology:link-id>link-1</ietf-network-topology:link-id>
      
      <ietf-network-topology:source>
        <ietf-network-topology:source-node>node-1</ietf-network-topology:source-node>
        <ietf-network-topology:source-tp>tp-1</ietf-network-topology:source-tp>
      </ietf-network-topology:source>
      
      <ietf-network-topology:destination>
        <ietf-network-topology:dest-node>node-2</ietf-network-topology:dest-node>
        <ietf-network-topology:dest-tp>tp-2</ietf-network-topology:dest-tp>
      </ietf-network-topology:destination>
      
      <ietf-network-topology:supporting-link>
        <ietf-network-topology:network-ref>supporting-network-1</ietf-network-topology:network-ref>
        <ietf-network-topology:link-ref>supporting-link-1</ietf-network-topology:link-ref>
      </ietf-network-topology:supporting-link>
      
      <device-layer:device-layer-link-attributes xmlns:device-layer="urn:ietf:params:xml:ns:yang:device-layer">
        <device-layer:physical-link-name>Link-1</device-layer:physical-link-name>
        <device-layer:physical-link-type>cat5e</device-layer:physical-link-type>
      </device-layer:device-layer-link-attributes>
      
      <network-layer:network-layer-link-attributes xmlns:network-layer="urn:ietf:params:xml:ns:yang:network-layer">
        <network-layer:link-name>Link-B</network-layer:link-name>
        <network-layer:link-type>3gpp-5g-qos-flow</network-layer:link-type>
        <network-layer:_5qi>9</network-layer:_5qi>
        <network-layer:gfbr>100</network-layer:gfbr>
        <network-layer:mfbr>200</network-layer:mfbr>
        <network-layer:pdb>10</network-layer:pdb>
        <network-layer:per>1</network-layer:per>
      </network-layer:network-layer-link-attributes>

    </ietf-network-topology:link>
  </network>
</networks>